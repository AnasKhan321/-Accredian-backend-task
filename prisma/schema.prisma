generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}


model detp{
  id      Int      @id @default(autoincrement())
  age     Int?   
  name   String? 
  
}



model refer {
  id          Int      @id @default(autoincrement())
  email       String
  phone_number String
  course_id   Int
  created_at   DateTime @default(now())
}
